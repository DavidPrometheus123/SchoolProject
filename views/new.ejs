        <h1 style="text-align: center"></h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/hjem" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="skill[name]" placeholder="navn">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="skill[author]" placeholder="forfattar">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="skill[description]" placeholder="tekst">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="skill[image]" placeholder="image">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit!</button>
                </div>
            </form>
        </div>
    </div>
</div>
